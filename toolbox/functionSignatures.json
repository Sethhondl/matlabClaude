{
    "_schemaVersion": "1.0.0",
    "derivux.launch": {
        "inputs": [],
        "outputs": [
            {
                "name": "app",
                "type": "derivux.DerivuxApp"
            }
        ],
        "description": "Launch Derivux assistant panel"
    },
    "derivux.DerivuxApp": {
        "inputs": [],
        "description": "Create Derivux application instance"
    },
    "derivux.DerivuxApp.launch": {
        "inputs": [],
        "description": "Open the Derivux window"
    },
    "derivux.DerivuxApp.close": {
        "inputs": [],
        "description": "Close the Derivux window"
    },
    "derivux.ClaudeProcessManager.sendMessage": {
        "inputs": [
            {
                "name": "prompt",
                "kind": "required",
                "type": ["char", "string"]
            },
            {
                "name": "allowedTools",
                "kind": "namevalue",
                "type": "cell"
            },
            {
                "name": "timeout",
                "kind": "namevalue",
                "type": "numeric"
            },
            {
                "name": "context",
                "kind": "namevalue",
                "type": ["char", "string"]
            }
        ],
        "description": "Send a message to Claude and get response"
    },
    "derivux.CodeExecutor.execute": {
        "inputs": [
            {
                "name": "code",
                "kind": "required",
                "type": ["char", "string"]
            }
        ],
        "outputs": [
            {
                "name": "result",
                "type": ["char", "string"]
            },
            {
                "name": "isError",
                "type": "logical"
            }
        ],
        "description": "Execute MATLAB code safely"
    },
    "derivux.SimulinkBridge.setCurrentModel": {
        "inputs": [
            {
                "name": "modelName",
                "kind": "required",
                "type": ["char", "string"]
            }
        ],
        "outputs": [
            {
                "name": "success",
                "type": "logical"
            }
        ],
        "description": "Set the current Simulink model"
    },
    "derivux.SimulinkBridge.addBlockFromLibrary": {
        "inputs": [
            {
                "name": "libraryPath",
                "kind": "required",
                "type": ["char", "string"]
            },
            {
                "name": "destName",
                "kind": "required",
                "type": ["char", "string"]
            },
            {
                "name": "params",
                "kind": "optional",
                "type": "struct"
            }
        ],
        "outputs": [
            {
                "name": "success",
                "type": "logical"
            }
        ],
        "description": "Add a block from Simulink library"
    },
    "derivux.config.Settings.load": {
        "inputs": [],
        "outputs": [
            {
                "name": "settings",
                "type": "derivux.config.Settings"
            }
        ],
        "description": "Load Derivux settings"
    }
}
